    "flow",
    "eslint",
    "babel",
    "express"
  ],
  "author": "Jesse Kernaghan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jessekernaghan/node-api/issues"
  },
  "homepage": "https://github.com/jessekernaghan/node-api#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "nconf": "^0.10.0"
  },
  "engines": {
    "node": "8.10.0"
  },
