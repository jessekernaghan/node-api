    "test": "jest",
    "build": "babel ./src --out-dir ./dist --ignore '**/*.spec.js'",
    "watch": "nodemon --exec babel-node -- src/index.js",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jessekernaghan/node-api.git"
  },
  "keywords": [
